doctype html
head
  meta(charset='UTF-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css' rel='stylesheet')
  link(rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css')
  link(rel='stylesheet' href='https://unicons.iconscout.com/release/v4.0.0/css/line.css')
  link(rel='stylesheet' href='css/ufregistration.css')
  link(rel='stylesheet' href='css/aodashboard.css')
  title Farmer One Dashboard
#wrapper.d-flex
  #sidebar-wrapper.bg-white
    .sidebar-heading.text-left.py-4.fs-4.fw-bold.text-uppercase.border-bottom Ufarm
    .list-group.list-group-flush.my-3
      a.list-group-item.list-group-item-action.bg-transparent.second-text.active(href='#')
      a.list-group-item.list-group-item-action.bg-transparent.second-text(href='/producelist') Uploaded Produce List
        div.form-row
          div.col.form-group
            a.btn.btn-success.text-white(class='button' href='/producelist') Back
  #page-content-wrapper
    nav.navbar.navbar-expand-lg.navbar-light.bg-transparent.py-4.px-4
      .d-flex.align-items-center
        h2.text-white #{currentUser.fullname} -
        h2.text-white #{currentUser.ward}
      form(method="post" action="/logout")
            button.btn.btn-success(type="submit") Log out
      button.navbar-toggler(type='button' data-bs-toggle='collapse' data-bs-target='#navbarSupportedContent' aria-controls='navbarSupportedContent' aria-expanded='false' aria-label='Toggle navigation')
        span.navbar-toggler-icon
    body 
        block content